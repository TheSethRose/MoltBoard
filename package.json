{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev -p 5278",
    "build": "next build",
    "start": "next start -p 5278",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --fix",
    "prettier": "prettier --write .",
    "clean": "npm run type-check && npm run lint && npm run prettier",
    "migrate": "node scripts/run-migrations.js",
    "worker": "node scripts/recurring-work.js",
    "worker:start": "pm2 start scripts/recurring-work.js --name dashboard-worker",
    "worker:stop": "pm2 stop dashboard-worker",
    "worker:logs": "pm2 logs dashboard-worker",
    "dashboard:start": "bash scripts/manage-launchctl.sh dashboard start",
    "dashboard:stop": "bash scripts/manage-launchctl.sh dashboard stop",
    "dashboard:restart": "bash scripts/manage-launchctl.sh dashboard restart",
    "dashboard:status": "bash scripts/manage-launchctl.sh dashboard status",
    "gateway:start": "bash scripts/manage-launchctl.sh gateway start",
    "gateway:stop": "bash scripts/manage-launchctl.sh gateway stop",
    "gateway:restart": "bash scripts/manage-launchctl.sh gateway restart",
    "gateway:status": "bash scripts/manage-launchctl.sh gateway status"
  },
  "dependencies": {
    "@mariozechner/pi-ai": "^0.50.3",
    "@octokit/rest": "^22.0.1",
    "@octokit/types": "^16.0.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-slot": "^1.2.4",
    "async-mutex": "^0.5.0",
    "better-sqlite3": "^12.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.29.0",
    "lucide-react": "^0.563.0",
    "next": "16.1.4",
    "next-themes": "^0.4.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-textarea-autosize": "^8.5.9",
    "sonner": "^2.0.7",
    "swr": "^2.3.8",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "bun-types": "^1.2.22",
    "eslint": "^9",
    "eslint-config-next": "16.1.4",
    "prettier": "^3.8.1",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
